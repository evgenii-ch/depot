{
  "name": "hardhat-project",
  "scripts": {
    "omnibus": "ts-node --files ./scripts/omnibs.ts",
    "omnibus:run": "hardhat omnibus:run",
    "omnibus:test": "hardhat omnibus:test",
    "omnibus:simulate": "hardhat omnibus:simulate",
    "omnibus:debug": "hardhat omnibus:simulate _example_omnibus",
    "omnibus:multi-test": "hardhat omnibus:multi-test",
    "ks:add": "hardhat keystore:add",
    "ks:ls": "hardhat keystore:list",
    "ks:del": "hardhat keystore:delete",
    "ks:gen": "hardhat keystore:generate",
    "ks:pwd": "hardhat keystore:password",
    "rpc:stop": "hardhat rpc:stop",
    "example:test:mainnet": "hardhat omnibus:test _example_tiny_omnibus",
    "example:test:holesky": "hardhat omnibus:test _example_tiny_holesky_omnibus",
    "example:run:holesky": "hardhat omnibus:run _example_tiny_holesky_omnibus --rpc remote --test-account false --network holesky",
    "example:multi-test:mainnet": "hardhat omnibus:multi-test _example_tiny_omnibus",
    "example:multi-test:scripts:mainnet": "hardhat omnibus:multi-test _example_tiny_omnibus --mount-tests true --repo scripts --hide-debug true",
    "example:multi-test:dg:mainnet": "hardhat omnibus:multi-test _example_tiny_omnibus --mount-tests true --repo dual-governance --hide-debug true",
    "example:multi-test:core:mainnet:custom": "hardhat omnibus:multi-test _example_tiny_omnibus --mount-tests true --repo core --pattern test/custom/_example_omnibus_test_for_core_repo.ts",
    "example:multi-test:scripts:mainnet:custom": "hardhat omnibus:multi-test _example_tiny_omnibus --mount-tests true --repo scripts --pattern tests/custom/_example_omnibus_test_for_scripts_repo.py",
    "example:multi-test:dg:mainnet:custom": "hardhat omnibus:multi-test _example_tiny_omnibus --mount-tests true --repo dual-governance --pattern test/custom/*",
    "example:multi-test:all:mainnet:custom": "hardhat omnibus:multi-test _example_tiny_omnibus --mount-tests true --pattern default --hide-debug true",
    "style": "prettier --write .",
    "test": "nyc --reporter text-summary mocha"
  },
  "devDependencies": {
    "@types/chai": "^4.3.14",
    "@types/chai-as-promised": "^7.1.8",
    "@types/clarinet": "^0.12.1",
    "@types/deep-eql": "^4.0.2",
    "@types/lodash": "^4.14.199",
    "@types/mocha": "^10.0.6",
    "@types/node-fetch": "^2.6.9",
    "@types/prompts": "^2.4.4",
    "@types/sinon": "^17.0.3",
    "chai-as-promised": "^7.1.1",
    "nock": "^13.5.0",
    "nyc": "^15.1.0",
    "prettier": "^3.2.5",
    "sinon": "^17.0.1",
    "ts-node": "^10.9.2",
    "viem": "^2.30.6"
  },
  "dependencies": {
    "@openzeppelin/contracts": "5.0.2",
    "@types/dockerode": "^3.3.32",
    "abitype": "^1.0.8",
    "chai": "^4.3.8",
    "chalk": "^4.1.2",
    "clarinet": "^0.12.6",
    "deep-eql": "4.1.4",
    "dockerode": "^4.0.2",
    "dotenv": "^16.3.1",
    "hardhat": "2.25.0",
    "ipfs-only-hash": "^4.0.0",
    "mocha": "^10.2.0",
    "nanospinner": "^1.2.2",
    "node-fetch": "^2.7.0",
    "pinata-web3": "^0.5.2",
    "prompts": "^2.4.2",
    "web3-eth-accounts": "^4.1.1",
    "web3-types": "^1.3.1"
  },
  "packageManager": "pnpm@9.15.0+sha512.76e2379760a4328ec4415815bcd6628dee727af3779aaa4c914e3944156c4299921a89f976381ee107d41f12cfa4b66681ca9c718f0668fa0831ed4c6d8ba56c"
}
